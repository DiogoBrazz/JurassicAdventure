{
  "multipart": [
    {
      "when": { "up": "true" },
      "apply": { "model": "jurassicadventure:block/electric_fence_post" }
    },
    {
      "when": { "north": "low" },
      "apply": { "model": "jurassicadventure:block/electric_fence_side", "uvlock": true }
    },
    {
      "when": { "east": "low" },
      "apply": { "model": "jurassicadventure:block/electric_fence_side", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "low" },
      "apply": { "model": "jurassicadventure:block/electric_fence_side", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "low" },
      "apply": { "model": "jurassicadventure:block/electric_fence_side", "y": 270, "uvlock": true }
    }
  ]
}
